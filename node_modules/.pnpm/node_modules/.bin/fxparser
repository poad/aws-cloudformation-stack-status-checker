#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ken-yo/git/poad/aws-cloudformation-stack-status-checker/node_modules/.pnpm/fast-xml-parser@4.2.4/node_modules/fast-xml-parser/src/cli/node_modules:/Users/ken-yo/git/poad/aws-cloudformation-stack-status-checker/node_modules/.pnpm/fast-xml-parser@4.2.4/node_modules/fast-xml-parser/src/node_modules:/Users/ken-yo/git/poad/aws-cloudformation-stack-status-checker/node_modules/.pnpm/fast-xml-parser@4.2.4/node_modules/fast-xml-parser/node_modules:/Users/ken-yo/git/poad/aws-cloudformation-stack-status-checker/node_modules/.pnpm/fast-xml-parser@4.2.4/node_modules:/Users/ken-yo/git/poad/aws-cloudformation-stack-status-checker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ken-yo/git/poad/aws-cloudformation-stack-status-checker/node_modules/.pnpm/fast-xml-parser@4.2.4/node_modules/fast-xml-parser/src/cli/node_modules:/Users/ken-yo/git/poad/aws-cloudformation-stack-status-checker/node_modules/.pnpm/fast-xml-parser@4.2.4/node_modules/fast-xml-parser/src/node_modules:/Users/ken-yo/git/poad/aws-cloudformation-stack-status-checker/node_modules/.pnpm/fast-xml-parser@4.2.4/node_modules/fast-xml-parser/node_modules:/Users/ken-yo/git/poad/aws-cloudformation-stack-status-checker/node_modules/.pnpm/fast-xml-parser@4.2.4/node_modules:/Users/ken-yo/git/poad/aws-cloudformation-stack-status-checker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fast-xml-parser/src/cli/cli.js" "$@"
else
  exec node  "$basedir/../fast-xml-parser/src/cli/cli.js" "$@"
fi
