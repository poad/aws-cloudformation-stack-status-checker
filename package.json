{
  "name": "aws-cloudformation-stack-status-checker",
  "version": "1.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.mjs",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.mts'",
    "format-check": "prettier --check '**/*.mts'",
    "lint": "eslint src/**/*.mts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/aws-cloudformation-stack-status-checker.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@aws-sdk/client-cloudformation": "^3.54.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/parser": "^5.15.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.11.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
