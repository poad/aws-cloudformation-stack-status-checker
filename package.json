{
  "name": "aws-cloudformation-stack-status-checker",
  "version": "1.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.mjs",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.mts'",
    "format-check": "prettier --check '**/*.mts'",
    "lint": "eslint src/**/*.mts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/aws-cloudformation-stack-status-checker.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "latest",
    "@aws-sdk/client-cloudformation": "^3.56.0"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@typescript-eslint/parser": "latest",
    "@vercel/ncc": "latest",
    "eslint": "latest",
    "eslint-plugin-github": "latest",
    "eslint-plugin-jest": "latest",
    "jest": "latest",
    "js-yaml": "latest",
    "prettier": "latest",
    "ts-jest": "latest",
    "typescript": "latest"
  }
}
